<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.xy.tomatotime">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/tomato"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.TomatoActivity"
            android:theme="@style/mainStyle"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.AddPlanActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.ReviseActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.AddScheduleActivity"
            android:label="@string/app_name" />

        <service
            android:name=".tool.AudioService"
            android:enabled="true"
            android:exported="true"></service>
        <service
            android:name=".tool.CountDownTimerService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>